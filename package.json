    "gitbook-cli": "^2.3.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "material-ui": "^0.15.4",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-server": "^2.1.0-beta.4"
  }
